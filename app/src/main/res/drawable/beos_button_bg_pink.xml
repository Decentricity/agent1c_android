<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#e6b3cc" />
            <stroke android:width="1dp" android:color="#7f5a6b" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="270"
                android:startColor="#ffd9eb"
                android:endColor="#efb8d4" />
            <stroke android:width="1dp" android:color="#8a6476" />
        </shape>
    </item>
</selector>
